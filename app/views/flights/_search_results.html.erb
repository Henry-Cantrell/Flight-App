<h2>Search results:</h2>
<%= @flight_search_results %>

<%= simple_form_for :flight_select, url: new_booking_path, method: 'GET' do |form| %>
    <%= form.input :id, :collection => @flight_search_results_names, :include_blank => false, :label => 'List of matching flights:' %> 
	<%= form.input :passenger_count, :collection => 1..4, :include_blank => false, :label => 'Number of passengers:' %>
    <%= form.submit %>
<% end %>
